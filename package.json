{
  "name": "rememberthetask-api",
  "version": "0.0.1",
  "description": "Api for RememberTheTask bot",
  "main": "./dist/main.js",
  "bin": "./dist/wwwrtt.js",
  "directories": {
    "doc": "doc",
    "example": "example"
  },
  "scripts": {
    "burstmake": "rimraf ./**/*.*~ && rimraf ./**/tsconfig.json && burstmake",
    "typedoc": "typedoc",
    "doc": "npm run typedoc -- --options typedoc.json --out ./doc/ ./src/ && apidoc -i src/ -o docAPI/",
    "clean": "rimraf ./dist/*",
    "build": "tsc -p ./src/",
    "rebuild": "npm run clean && npm run build",
    "dev-launch": "npm run rebuild && npm test && npm start",
    "pretest": "tsc -p ./test/",
    "test": "npm run lint && mocha ./test/*.js",
    "lint": "tslint ./{src,test}/**/*.ts",
    "typings": "typings install",
    "start": "node ./dist/wwwrtt.js",
    "prepublish": "npm run burstmake && npm run typings && npm run build && rimraf /dist/**/*.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BugBusterSWE/RememberTheTask-api"
  },
  "bugs": {
    "url": "https://github.com/BugBusterSWE/RememberTheTask-api/issues"
  },
  "keywords": [
    "api",
    "bot",
    "rememberthetask"
  ],
  "author": "Davide Polonio poloniodavide@gmail.com",
  "license": "MIT",
  "dependencies": {
    "express": "^4.13.1"
  },
  "devDependencies": {
    "apidoc": "^0.15.1",
    "burstmake": "latest",
    "chai": "^3.5.0",
    "dependency-check": "^2.5.1",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
    "tslint": "^3.5.0",
    "typedoc": "^0.3.12",
    "typescript": "^1.8.7",
    "typings": "^0.7.9"
  }
}
